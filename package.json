    "@lingui/macro": "^3.14.0",
    "@prettier/plugin-xml": "^0.13.1",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/babel__core": "~7.1.14",
    "@types/babel-plugin-macros": "^2.8.5",
    "@types/eslint": "^7.2.13",
    "@types/eslint-plugin-prettier": "^3.1.0",
