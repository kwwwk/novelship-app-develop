{
  "name": "novelship-app",
  "version": "1.24.3",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "dev": "react-native start",
    "dev:api-tunnel": "ngrok http -bind-tls=true -inspect=false https://localhost:3000",
    "android": "react-native run-android --appIdSuffix 'dev'",
    "android:release": "npx react-native run-android --variant=release",
    "android:staging": "npx react-native run-android --variant=stagingrelease --appIdSuffix 'test'",
    "android:release:build": "cd android && npm run lingui:import && ENVFILE=.env.production ./gradlew bundleRelease",
    "android:staging:build": "cd android && npm run lingui:import && ENVFILE=.env.staging ./gradlew bundleStagingrelease",
    "ios": "react-native run-ios",
    "test": "jest --watchAll",
    "prettier": "prettier --write . --loglevel silent",
    "eslint": "eslint . --fix",
    "pretty-quick": "pretty-quick",
    "lint": "tsc && eslint . --fix && pretty-quick --staged",
    "lint:all": "tsc && eslint . --fix && pretty-quick",
    "tsc": "tsc",
    "types:install": "npx typesync",
    "depcheck": "npx depcheck",
    "clean": "cd android && ./gradlew clean && cd ..",
    "link-assets": "npx react-native-asset",
    "postversion": "react-native-version",
    "postinstall": "patch-package",
    "lingui:extract": "lingui extract --clean",
    "lingui:compile": "lingui compile",
    "lingui:import": "node scripts/import-translations && lingui compile"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4.1.5",
    "@invertase/react-native-apple-authentication": "^2.1.5",
    "@lingui/react": "^3.14.0",
